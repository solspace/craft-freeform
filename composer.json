{
  "name": "solspace/craft3-freeform",
  "description": "The most intuitive and powerful form builder for Craft.",
  "version": "2.5.13",
  "type": "craft-plugin",
  "authors": [
    {
      "name": "Solspace",
      "homepage": "https://solspace.com/"
    }
