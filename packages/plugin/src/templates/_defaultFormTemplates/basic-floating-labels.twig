        .freeform-col-5 { width: 41.66%; }
        .freeform-col-6 { width: 50%; }
        .freeform-col-7 { width: 58.33%; }
        .freeform-col-8 { width: 66.66%; }
        .freeform-col-9 { width: 75%; }
        .freeform-col-10 { width: 83.33%; }
        .freeform-col-11 { width: 91.66%; }
