    "symfony/finder": "^2.8|^3.0|^4.0",
    "symfony/filesystem": "^2.8|^3.0|^4.0",
    "solspace/craft3-commons": "^1.0.17",
    "hashids/hashids": "^2.0",
    "pixel418/markdownify": "^2.3.1",
