{
  "name": "@ff/client",
  "version": "1.0.0",
  "description": "Freeform Client",
  "scripts": {
    "dev": "npm run clean && NODE_ENV=development npm run watch",
    "build": "npm run clean && NODE_ENV=production npm run compile",
    "watch": "webpack serve --config ./config/webpack/dev.config.js",
    "clean": "rm -rf ../plugin/src/Resources/js/client",
    "compile": "webpack --config ./config/webpack/prod.config.js",
    "lint": "eslint -c .eslintrc.json .",
    "lint:fix": "eslint --fix -c .eslintrc.json .",
    "test": "jest",
    "test:coverage": "jest --maxWorkers=4 --collect-coverage",
    "test:watch": "jest --watch --maxWorkers=4"
  },
  "keywords": [
    "Freeform",
    "Craft CMS",
    "App"
  ],
  "author": "Solspace Inc.",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "axios": "^0.27.2",
    "browserslist": "^4.20.3",
    "concurrently": "^7.4.0",
    "date-fns": "^2.29.3",
    "jest": "^29.2.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.foreach": "^4.5.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.template": "^4.5.0",
    "path-parse": "^1.0.7",
    "pubsub-js": "^1.9.4",
    "react": "^18.1.0",
    "react-color": "^2.19.3",
    "react-datepicker": "^4.8.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.1.0",
    "react-loading-skeleton": "^3.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.3",
    "react-spring": "^9.4.5",
    "react-tippy": "^1.4.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5",
    "transliteration": "^2.3.5",
    "unique-names-generator": "^4.7.1",
    "url-parse": "^1.5.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@redux-devtools/core": "^3.13.1",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/lodash.template": "^4.5.1",
    "@types/node": "^18.11.3",
    "@types/pubsub-js": "^1.8.3",
    "@types/react": "^18.0.9",
    "@types/react-color": "^3.0.6",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.4",
    "@types/styled-components": "^5.1.25",
    "@types/url-parse": "^1.4.8",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "autoprefixer": "^10.4.7",
    "css-loader": "^6.7.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.2.1",
    "jsdom": "^20.0.1",
    "jsdom-global": "^3.0.2",
    "msw": "^0.47.4",
    "react-refresh": "^0.13.0",
    "react-refresh-typescript": "^2.0.5",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript-plugin-styled-components": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "last 2 versions"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      ".(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "./config/jest/setup.ts"
    ],
    "testMatch": [
      "**/*.test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss|styl)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/__mocks__/file-mock.ts",
      "\\.(svg)$": "<rootDir>/config/jest/__mocks__/svg-mock.tsx",
      "^@ff-client/(.*)": "<rootDir>/src/$1",
      "^@editor/(.*)": "<rootDir>/src/app/pages/forms/edit/$1",
      "^@components/(.*)": "<rootDir>/src/app/components/$1",
      "^@ff-icons/(.*)": "<rootDir>/src/assets/icons/$1"
    }
  }
}
